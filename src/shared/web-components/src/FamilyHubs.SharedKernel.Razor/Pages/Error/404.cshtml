@page
@model FamilyHubs.SharedKernel.Razor.Pages.Error.ErrorModel
@{
    ViewData["Title"] = "Page not found";
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <h1 class="govuk-heading-l">Page not found</h1>
        <p>The page you are looking for may have been removed.</p>
        <p>If you have checked the web address is correct or you selected a link or button, <a asp-page="/Contact-Us/Index">contact us</a>.</p>

        <a asp-page="/Index" role="button" draggable="false" class="govuk-button govuk-!-margin-top-2" data-module="govuk-button">
            Go to homepage
        </a>
    </div>
</div>
